generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model ApiKey {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  key       String   @unique
  name      String?
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
  lastUsed  DateTime?
  expiresAt DateTime?
}

model Subproject {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  code String @unique
  title String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}